{
  "name": "rescript-json-schema-online",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "rescript-schema",
    "rescript-json-schema"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bsc-flags": ["-open RescriptCore", "-open RescriptSchema"]
}
